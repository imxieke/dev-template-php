{
  "name": "imxieke/php-template",
  "description": "PHP Project Template",
  "type": "project",
  "license": "MIT",
  "homepage": "https://github.com/imxieke/php-template",
  "keywords": [
    "php",
    "project",
    "template"
  ],
  "authors": [
    {
      "name": "Cloudflying",
      "email": "oss@live.hk",
      "homepage": "https://github.com/imxieke",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4.0"
  },
  "require-dev": {
    "filp/whoops": "^2.14",
    "mockery/mockery": "^1.4",
    "phpunit/phpunit": "^9.0",
    "monolog/monolog": "^2.3",
    "phpstan/phpstan": "^1.3",
    "symfony/var-dumper": "^5.4",
    "vimeo/psalm": "^4.16"
  },
  "scripts": {
    "test": "phpunit --colors",
    "psalm": "psalm --show-info=true --no-cache",
    "psalm-fix": "psalm --no-cache --alter --issues=MissingReturnType,MissingParamType"
  },
  "scripts-descriptions": {
    "test": "Run all tests."
  }
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "process-timeout": 300,
    "optimize-autoloader": true
  }
}